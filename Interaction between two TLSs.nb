 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "int"], "=", 
    RowBox[{"\[HBar]", " ", "\[Gamma]", " ", 
     RowBox[{"(", 
      RowBox[{"|", 
       RowBox[{
        SubscriptBox["e", "1"], ">", "<", 
        SubscriptBox["g", "1"]}], "|", 
       RowBox[{"\[CircleTimes]", 
        RowBox[{"|", 
         RowBox[{
          SubscriptBox["g", "2"], ">", "<", 
          SubscriptBox["e", "2"]}], "|", 
         RowBox[{"+", 
          RowBox[{"|", 
           RowBox[{
            SubscriptBox["g", "1"], ">", "<", 
            SubscriptBox["e", "1"]}], "|", 
           RowBox[{"\[CircleTimes]", 
            RowBox[{"|", 
             RowBox[{
              SubscriptBox["e", "2"], ">", "<", 
              SubscriptBox["g", "2"]}], "|"}]}]}]}]}]}]}], ")"}]}]}], 
